#ifndef CEXCEPTION_H_
#define CEXCEPTION_H_

#include <exception>
#include <string>

class CException: public std::exception
{
public:
	CException();
	CException(std::string msg);
	~CException() throw(){};
};

#endif /* CEXCEPTION_H_ */
